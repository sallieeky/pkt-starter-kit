{
    "name": "pkt/starter-kit",
    "description": "Comprehensive and efficient toolkit designed to streamline the process of developing Laravel packages In Pupuk Kaltim",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Pkt\\StarterKit\\": "src/"
        }
    },
    "authors": [
        {
            "name": "sallieeky",
            "email": "sallieeky@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1.0",
        "illuminate/console": "^10.17",
        "illuminate/filesystem": "^10.17",
        "illuminate/support": "^10.17",
        "illuminate/validation": "^10.17"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "dev-dev"
        },
        "laravel": {
            "providers": [
                "Pkt\\StarterKit\\StarterKitServiceProvider"
            ]
        }
    }
}
