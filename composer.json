{
    "name": "pkt/starter-kit",
    "description": "Comprehensive and efficient toolkit designed to streamline the process of developing Laravel application In PT Pupuk Kaltim",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Pkt\\StarterKit\\": "src/"
        }
    },
    "authors": [
        {
            "name": "sallieeky",
            "email": "sallieeky@gmail.com"
        },
        {
            "name": "ardmgtm",
            "email": "muhamad.ardian24@gmail.com"
        },
        {
            "name": "sallixie",
            "email": "10191077@student.itk.ac.id"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1.0",
        "illuminate/console": "^10.17",
        "illuminate/filesystem": "^10.17",
        "illuminate/support": "^10.17",
        "illuminate/validation": "^10.17",
        "illuminate/database": "^10.0",
        "illuminate/bus": "*",
        "illuminate/notifications": "*",
        "doctrine/dbal": "*",
        "illuminate/routing": "10.x-dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "dev-dev"
        },
        "laravel": {
            "providers": [
                "Pkt\\StarterKit\\StarterKitServiceProvider",
                "Pkt\\StarterKit\\StarterKitMacroServiceProvider"
            ]
        }
    }
}
